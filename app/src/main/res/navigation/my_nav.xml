<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/my_nav"
    app:startDestination="@id/loginFragment2">

    <fragment
        android:id="@+id/registerFragment"
        android:name="com.example.pmordo.presentation.firstscreen.register.RegisterFragment"
        android:label="RegisterFragment" >
        <action
            android:id="@+id/action_registerFragment_to_createPasswordFragment"
            app:destination="@id/createPasswordFragment" />
        <action
            android:id="@+id/action_registerFragment_to_loginFragment2"
            app:destination="@id/loginFragment2" />
        <action
            android:id="@+id/action_registerFragment_to_profileMainFragment"
            app:destination="@id/profileMainFragment" />
    </fragment>
    <fragment
        android:id="@+id/createPasswordFragment"
        android:name="com.example.pmordo.presentation.firstscreen.register.CreatePasswordFragment"
        android:label="CreatePasswordFragment" >
        <action
            android:id="@+id/action_createPasswordFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_createPasswordFragment_to_profileMainFragment"
            app:destination="@id/profileMainFragment" />
    </fragment>
    <fragment
        android:id="@+id/changeProfileFragment"
        android:name="com.example.pmordo.fragments.ChangeProfileFragment"
        android:label="ChangeProfileFragment" >
        <action
            android:id="@+id/action_changeProfileFragment_to_profileMainFragment2"
            app:destination="@id/profileMainFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileMainFragment"
        android:name="com.example.pmordo.fragments.ProfileMainFragment"
        android:label="ProfileMainFragment" >
        <action
            android:id="@+id/action_profileMainFragment_to_changeProfileFragment"
            app:destination="@id/changeProfileFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment2"
        android:name="com.example.pmordo.presentation.firstscreen.login.LoginFragment"
        android:label="LoginFragment" >
        <action
            android:id="@+id/action_loginFragment2_to_registerFragment"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_loginFragment2_to_profileMainFragment"
            app:destination="@id/profileMainFragment" />
        <action
            android:id="@+id/action_loginFragment2_to_greetingFragment"
            app:destination="@id/greetingFragment" />
    </fragment>
    <fragment
        android:id="@+id/greetingFragment"
        android:name="com.example.pmordo.presentation.firstscreen.register.GreetingFragment"
        android:label="GreetingFragment" >
        <action
            android:id="@+id/action_greetingFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_greetingFragment_to_sellerRegisterFragment"
            app:destination="@id/sellerRegisterFragment" />
    </fragment>
    <fragment
        android:id="@+id/sellerRegisterFragment"
        android:name="com.example.pmordo.presentation.firstscreen.register.SellerRegisterFragment"
        android:label="SellerRegisterFragment" />
</navigation>